{
  "name": "ventur",
  "version": "1.4.6",
  "description": "Ventur App",
  "private": true,
  "author": "Luiz Gon√ßalves <luizhbgoncalves@gmail.com>",
  "license": "MIT",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "npm run typecheck && node scripts/build.mjs && electron-builder --config .electron-builder.config.js",
    "typecheck": "tsc --noEmit --project packages/renderer/tsconfig.json",
    "debug": "cross-env-shell NODE_ENV=debug \"npm run typecheck && node scripts/build.mjs && vite ./packages/renderer\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "date-fns": "^2.28.0",
    "electron-store": "^8.0.1",
    "framer-motion": "^6.2.8",
    "nedb-promises": "^6.0.3",
    "react-day-picker": "^7.4.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@lukemorales/eslint-config": "1.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "@vitejs/plugin-react": "^1.2.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-10x": "1.5.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jest-formatting": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-radar": "0.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-redux": "3.3.2",
    "eslint-plugin-testing-library": "3.10.1",
    "husky": "^7.0.4",
    "prettier": "2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "sass": "^1.49.7",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "vite-plugin-resolve": "^1.4.4"
  },
  "env": {
    "//": "Used in build scripts",
    "PORT": 3344
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
