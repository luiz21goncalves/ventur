{
  "name": "ventur",
  "version": "1.4.6",
  "description": "Ventur App",
  "main": "./out/main/index.js",
  "author": "Luiz Gon√ßalves <luizhbgoncalves@gmail.com>",
  "homepage": "ventur.luizgoncalves.com",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck": "tsc --noEmit --composite false",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "test": "vitest --run",
    "test:cov": "vitest --run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.8",
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "dayjs": "^1.11.7",
    "dexie": "^3.2.3",
    "electron-router-dom": "^1.0.5",
    "electron-store": "^8.1.0",
    "framer-motion": "^8.4.3",
    "jotai": "^1.13.0",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@faker-js/faker": "^7.6.0",
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/node": "16.18.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.1",
    "electron": "^21.3.3",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.17",
    "eslint": "^8.31.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "factory.ts": "^1.3.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.27.1"
  }
